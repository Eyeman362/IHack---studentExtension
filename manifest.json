{
    "manifest_version": 3,
    "name": "Student Resources",
    "version": "1.0",
    "description": "An extension to aid students by offering specific resources.",
    "permissions": [
      "activeTab","contextMenus","tabs"
    ],
    "action": {
      "default_popup": "./popup/popup.html"
    },
    "background": {
      "service_worker": "./background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
    
  }
  